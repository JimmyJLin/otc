<% layout('main') -%>
<div id="page_title">
  <h1>Powdered</h1>
</div>

<div id="powdered">
  <div id="powdered_background"></div>
  <div class="container-fluid">
    <div id="h_line" class="center-block"></div>
    
    <h2><%= data.seminars[1].title %></h2>
    <h4><%= data.seminars[1].type %> - <%= data.seminars[1].length %></h4>
    <p><%= data.seminars[1].description %></p>
    <div class="row">
      <img src='<%= data.seminars[1].img_url%>' alt="image">
    </div>
    <p><%= data.seminars[1].additional_info %></p>
    <div id="benefits">
      <p>
        <span><%= data.seminars[1].model %></span>
        <br>
        <%= data.seminars[1].model_details %>
      </p>
      <p>
        <span><%= data.seminars[1].starter_kit %></span>
        <br>
        <%= data.seminars[1].starter_kit_details %>
      </p>
      <p>
        <span><%= data.seminars[1].benefits %></span>
        <br>
        <%= data.seminars[1].benefits_details %>
      </p>
      <h3>Fee: $<%= data.seminars[1].fee %></h3>

      <a href="/register" class="btn btn-default center-block"> Get More Info </a>
    </div>
  </div>
</div>
